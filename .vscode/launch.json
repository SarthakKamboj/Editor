    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Editor",
            "type": "cppdbg",
            "preLaunchTask": "Build_Editor",
            "request": "launch",
            "program": "${workspaceFolder}/build/editor.exe",
            "args": [],
            "windows": {
                "MIMode": "gdb"
            },
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "logging": {
                "moduleLoad": false,
                "trace": true
            },
            // "visualizerFile": "${workspaceFolder}/my.natvis",
            // "showDisplayString": true
        }
    ]
}